{
  "name": "nextjs-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:prod": "env-cmd -f .env.production next dev",
    "build": "next build",
    "start:dev": "env-cmd -f .env.development next start",
    "start:staging": "env-cmd -f .env.staging next start",
    "start:prod": "next start",
    "lint": "eslint \"*.{ts,tsx,js,jsx}\" --color",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "prettier:check": "prettier --write .prettierrc.js --check \"*.{ts,js,tsx,jsx,scss,css,html}\"",
    "sitemap": "next-sitemap --config next-sitemap.config.js",
    "start": "next start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --color",
      "npm run prettier:check",
      "git add ."
    ]
  },
  "dependencies": {
    "@platform/util.local-storage": "^0.0.137",
    "@react-oauth/google": "^0.12.1",
    "@reduxjs/toolkit": "^1.8.0",
    "@types/jwt-decode": "^3.1.0",
    "@types/user-agents": "^1.0.4",
    "@zeit/next-sass": "^1.0.1",
    "antd": "^4.18.9",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "ethers": "^5.4.3",
    "formik": "^2.2.9",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "next": "12.1.2",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^8.1.3",
    "next-images": "1.8.4",
    "next-redux-wrapper": "7.0.5",
    "next-seo": "^4.24.0",
    "next-sitemap": "2.1.15",
    "nextjs-progressbar": "^0.0.16",
    "react": "^17.0.2",
    "react-dom": "17.0.2",
    "react-oauth-google": "^0.9.0",
    "react-redux": "^7.2.8",
    "react-scroll": "^1.8.7",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "sass": "^1.49.9",
    "socket.io-client": "^4.7.2",
    "swr": "^1.2.2",
    "web3": "^1.3.6",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.21",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@types/react-scroll": "^1.8.4",
    "@types/socket.io-client": "^3.0.0",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.2.1",
    "typescript": "^5.3.2"
  }
}
